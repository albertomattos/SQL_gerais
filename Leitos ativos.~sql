select t.*, t.rowid from faleicad t where 
t.leito not like 'P%' and t.leito not like 'U%' and t.leito not like 'R%' and t.leito not like 'V%' and t.leito not like 'H%'
and t.leito not like 'B%'
and t.desativado is null
/*--
select t.*, t.rowid from limpeza_leito t where t.leito like '%113%'
order by 1, 2 desc
--
select t.*, t.rowid from tb_leito_periodo t where t.leito like '%111%'
*/
